{
  "extends": ["google", "prettier"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "plugins": ["jasmine", "prettier"],
  "globals": {
    "Plotly": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jasmine": true
  }
}
